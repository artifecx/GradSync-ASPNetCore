<div class="modal fade" id="deleteModal" tabindex="-1">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header border-0">
                <h5 class="modal-title flex items-center text-[#343131]" id="deleteModalLabel">
                    @Html.Raw(Resources.Constants.SvgIcons.ModalDeleteIcon)
                    Confirm Delete
                </h5>
                <button type="button" class="close" data-dismiss="modal">
                    <span>&times;</span>
                </button>
            </div>
            <div class="modal-body mt-2 py-1 pl-4 text-[#343131]" id="deleteModalMessage">
                Are you sure you want to delete this item?
            </div>
            <div class="modal-footer mt-2 !border-none">
                <button type="button" class="min-w-[128px] rounded-md bg-[#6c757d] px-3 py-2 font-medium text-white hover:bg-[#939BA2]" data-dismiss="modal">Cancel</button>
                <button type="button" class="min-w-[128px] rounded-md bg-[#7A1515] px-3 py-2 font-medium text-white hover:bg-[#A53D3D] disabled:bg-gray-400" id="confirmDeleteBtn">Delete</button>
            </div>
        </div>
    </div>
</div>