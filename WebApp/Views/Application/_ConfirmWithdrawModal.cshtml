<div class="modal fade" id="withdrawApplicationModal" tabindex="-1" role="dialog" aria-labelledby="withdrawApplicationModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header border-0">
                <h5 class="modal-title flex items-center text-[#343131]" id="withdrawApplicationModalLabel">
                    @Html.Raw(Resources.Constants.SvgIcons.ModalDeleteIcon)
                    Confirm Withdrawal
                </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="closeWithdrawApplicationModal">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body mt-2 py-1 pl-4 text-[#343131]">
                Are you sure you want to withdraw the application for the position of <span class="font-semibold" id="withdrawPositionTitle"></span>?
                <div class="hidden" id="withdrawApplicationId"></div>
            </div>

            <div class="modal-footer border-0 mt-2">
                <button type="button" class="min-w-[128px] rounded-md bg-[#6c757d] px-3 py-2 font-medium text-white hover:bg-[#939BA2]" id="cancelWithdrawApplication" data-dismiss="modal">Cancel</button>
                <button type="submit" class="min-w-[128px] rounded-md bg-[#7A1515] px-3 py-2 font-medium text-white hover:bg-[#A04747] disabled:bg-gray-400" id="submitWithdrawApplication">Withdraw</button>
            </div>
        </div>
    </div>
</div>